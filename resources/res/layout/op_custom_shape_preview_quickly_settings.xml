<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:id="@+id/layout_container" android:layout_width="match_parent" android:layout_height="match_parent">
    <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
    <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="8">
        <LinearLayout android:orientation="vertical" android:id="@+id/quickly_settings_group" android:background="@drawable/op_theme_shape_preview_round" android:layout_width="297dp" android:layout_height="0dp" android:layout_weight="1">
            <LinearLayout android:gravity="center_vertical" android:layout_gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent">
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
                <com.oneplus.settings.ui.OPSquarelLinearLayout android:gravity="center" android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/adaptive_wifi" android:layout_width="0dp" android:layout_height="wrap_content" android:src="@drawable/ic_op_shape_squircle_enable" android:layout_weight="2">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_op_shape_wifi_for_adaptive"/>
                </com.oneplus.settings.ui.OPSquarelLinearLayout>
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
                <com.oneplus.settings.ui.OPSquarelLinearLayout android:gravity="center" android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/adaptive_flashlight" android:background="@drawable/ic_op_shape_squircle_disable" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="2">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_op_shape_flashlight"/>
                </com.oneplus.settings.ui.OPSquarelLinearLayout>
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
                <com.oneplus.settings.ui.OPSquarelLinearLayout android:gravity="center" android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/adaptive_nfc" android:background="@drawable/ic_op_shape_squircle_enable" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="2">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_op_shape_nfc"/>
                </com.oneplus.settings.ui.OPSquarelLinearLayout>
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
                <com.oneplus.settings.ui.OPSquarelLinearLayout android:gravity="center" android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/adaptive_airplane" android:background="@drawable/ic_op_shape_squircle_disable" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="2">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_op_shape_airplane"/>
                </com.oneplus.settings.ui.OPSquarelLinearLayout>
                <View android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/notification_group" android:background="@drawable/op_theme_shape_preview_round" android:layout_width="297dp" android:layout_height="0dp" android:layout_marginTop="8dp" android:layout_weight="2">
            <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
            <ImageView android:layout_width="match_parent" android:layout_height="0dp" android:src="@drawable/ic_op_theme_shape_notification" android:scaleType="fitXY" android:layout_weight="4" android:layout_marginStart="21dp" android:layout_marginEnd="21dp"/>
            <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
            <ImageView android:background="@color/op_control_divider_color_default" android:layout_width="match_parent" android:layout_height="@dimen/op_control_divider_height_standard"/>
            <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
            <ImageView android:layout_width="match_parent" android:layout_height="0dp" android:src="@drawable/ic_op_theme_shape_notification" android:scaleType="fitXY" android:layout_weight="4" android:layout_marginStart="21dp" android:layout_marginEnd="21dp"/>
            <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
    <View android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
</LinearLayout>
