<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
    <ImageView android:layout_gravity="center_horizontal" android:id="@+id/always_show_nb_anim" android:visibility="gone" android:layout_width="97dp" android:layout_height="202dp" android:layout_marginTop="30dp" android:layout_marginBottom="18dp"/>
    <LinearLayout android:orientation="vertical" android:id="@+id/gesture_anim_viewpager_container" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
        <androidx.viewpager.widget.ViewPager android:layout_gravity="center" android:id="@+id/gesture_anim_viewpager" android:layout_width="match_parent" android:layout_height="420dp"/>
        <LinearLayout android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content">
            <com.google.android.material.indicator.PageIndicator android:orientation="horizontal" android:id="@+id/gesture_page_indicator" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
